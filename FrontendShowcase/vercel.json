{
  "version": 2,
  "builds": [
    {
      "src": "server/index.ts",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "server/index.ts": {
      "includeFiles": [
        "dist/public/**",
        "shared/**",
        "server/**"
      ]
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/server/index.ts"
    }
  ],
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dist"
}
